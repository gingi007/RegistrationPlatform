<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title><%= eventname %> Registration</title>

    <!-- CSS -->
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Roboto:400,100,300,500">
    <link rel="stylesheet" href="assets/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="assets/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="assets/css/form-elements.css">
    <link rel="stylesheet" href="assets/css/style.css">
    <link rel="stylesheet" href="assets/css/jquery.qtip.css"/>

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>

    <![endif]-->

    <!-- Favicon and touch icons -->
    <link rel="shortcut icon" href="assets/ico/favicon.png">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="assets/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="assets/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="assets/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="assets/ico/apple-touch-icon-57-precomposed.png">

</head>

<body>

<!-- Top menu -->
<nav class="navbar navbar-inverse navbar-no-bg" role="navigation">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#top-navbar-1">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a href="/"><img class="logo-hack" src="/assets/img/mini-logo.png" alt=""/></a>
        </div>
        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse" id="top-navbar-1">
            <ul class="nav navbar-nav navbar-right">
                <li>
                    <a href="<%= eventwebsite %>"><strong><%= eventname %></strong></a>
							<span class="li-social">
								<a href="<%= eventfacebook %>"><i class="fa fa-facebook"></i></a>

							</span>
                </li>
            </ul>
        </div>
    </div>
</nav>

<!-- Top content -->
<div class="top-content">

    <div class="inner-bg">

        <div class="row">
            <div class="col-sm-12 text">
                <h1><strong><%= eventname %></strong> Registered Users</h1>
                <div class="description">
                </div>
            </div>
        </div>
        <div class="row">
            <div style="margin: 40px;">
                <table style="background-color: white">
                    <tr>
                        <td>name</td>
                        <td>email</td>
                        <td>phone</td>
                        <td>institute</td>
                        <td>degree</td>
                        <td>size</td>
                        <td>food</td>
                        <td>skills</td>
                        <td>accepted?</td>
                        <td>in team?</td>
                        <td>team id</td>
                    </tr>

                    <% for(var i = 0; i < users.length; i++) { %>
                    <tr>
                        <td><input disabled class="printInputText"
                                   value="<%= users[i].first_name + " " + users[i].last_name %>" id="fname"
                                   type="text"/></td>
                        <td><input disabled class="printInputText" value="<%= users[i].email %>" type="text"
                                   id="email"/></td>
                        <td><input disabled class="printInputText" value="<%= users[i].phone %>" type="text"
                                   id="phone"/></td>
                        <td><input disabled class="printInputText" value="<%= users[i].inst %>" type="text" id="inst"/>
                        </td>
                        <td><input disabled class="printInputText" value="<%= users[i].degree %>" type="text" id="deg"/>
                        </td>
                        <td><input disabled class="printInputText" value="<%= users[i].tshirt %>" type="text"
                                   id="size"/></td>
                        <td><input disabled class="printInputText" value="<%= users[i].food %>" type="text" id="food"/>
                        </td>
                        <td><select name="skills" id="skills">
                                <% if (users[i].tags){ %>
                                <% Object.keys(users[i].tags).forEach(function(key) { %>
                                <option><%= users[i].tags[key] %> </option>
                                <% })} %>
                            </select>
                        </td>
                        <td><input id="<%= users[i]._id %>" type="checkbox" name="isAccepted"
                                    <% if (users[i].accepted) { %>
                                   checked
                                    <% } %>
                                    />
                        </td>
                        <td><input data-uid="<%= users[i]._id %>" type="checkbox" name="isMember"
                                    <% if (users[i].isMember) { %>
                                   checked
                                    <% } %>
                                    />
                        </td>
                        <td><input disabled class="printInputText" value="<%= users[i].team %>" type="text" id="team"/>
                        </td>
                    </tr>
                    <% } %>
                </table>
            </div>

        </div>


    </div>

</div>


<!-- Javascript -->
<script src="assets/js/jquery-1.11.3.min.js"></script>
<script src="assets/bootstrap/js/bootstrap.min.js"></script>
<script src="assets/js/jquery.backstretch.min.js"></script>
<script src="assets/js/retina-1.1.0.min.js"></script>
<script src="assets/js/scripts.js"></script>
<script src="assets/js/jquery.qtip.js"></script>

<!--[if lt IE 10]>
<script src="assets/js/placeholder.js"></script>
<![endif]-->

</body>

</html>